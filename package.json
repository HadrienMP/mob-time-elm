{
	"name": "mob-time-elm",
	"version": "1.0.10",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node server/app.js",
		"dev:all": "concurrently --kill-others \"npm run dev:build-front\" \"npm run dev:server\"",
		"dev:build-front": "elm-live src/elm/Main.elm --dir=./public/js --no-server -- --output=./public/js/main.js --debug",
		"dev:server": "nodemon server/app.js",
		"release": "./dist.sh && git status && release-it 1.0.10 --ci",
		"test": "npx elm-test --watch \"./src/elm/**/*Spec.elm\""
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"concurrently": "^5.3.0",
		"elm-live": "^4.0.2",
		"https-local": "^1.0.0",
		"nodemon": "^2.0.7",
		"release-it": "^14.4.1"
	},
	"release-it": {
		"npm": {
			"publish": false
		},
		"github": {
			"release": true,
			"assets": [
				"./dist/dist.zip"
			]
		}
	},
	"dependencies": {
		"elm-test": "^0.19.1-revision6",
		"express": "^4.17.1",
		"socket.io": "^3.1.2"
	}
}
